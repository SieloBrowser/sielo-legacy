/**
 * Tab bar & tab rules
 */
QTabBar {
  border-bottom: 1px solid #080809;
  background: #0c0c0d;
}

QTabBar::tab
{
  height: 33px;
  margin: 0px;
  padding: 0px 6px;
  color: #ffffff;
}

QTabBar::tab:selected
{
  border-top: 3px solid scolor(accent);
  border-left: 1px solid #535355;
  border-right: 1px solid #535355;
  border-bottom: none;
  background: #323234;
}

QTabBar::tab:!selected
{
  border-right: 1px solid #535355;
  height: 23px;
  margin: 5px 0px 5px 0px;
  background: transparent
}

QTabBar::tab:!selected:hover
{
  height: 33px;
  margin: 0px;
  border-top: 3px solid #a3a4a6;
  background: #252526 repeat-x;
}

#tabbarwidget * {
  height: 35px;
  background: #0c0c0d;
}

#maintabbar {
  border-bottom: 3px solid #080809;
}

#maintabbar::tab {
  height: 37px;
  margin: 0px;
  padding: 0px 6px;
  color: #ffffff;
}


#maintabbar::tab:!selected
{
  border-right: 1px solid #535355;
  height: 27px;
  margin: 5px 0px 6px 0px;
  background: transparent
}

#maintabbar::tab:!selected:hover
{
  height: 37px;
  margin: 0px;
  border-top: 3px solid #a3a4a6;
  border-right: 1px solid #535355;
  border-left: 1px solid #535355;
  background: #252526 repeat-x;
}


/**
 * Add & close tab & list tab button rules
 */

#tabwidget-button-addtab,
#tabwidget-button-listtabs,
#tabwidget-button-mainmenu {
    border-radius: 5px;
    max-height: 30px;
    max-width: 30px;
    width: 30px;
    height: 30px;
    margin-left: 3px;
}

#tabwidget-button-addtab:hover,
#tabwidget-button-listtabs:hover,
#tabwidget-button-mainmenu:hover {
    border: #252526;
    background: #252526;
}

